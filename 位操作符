int main()
{
	////& - 按2进制位与 - 按位与
	//int a = 3;
	//int b = 5;
	//int c = a & b;
	////00000000000000000000000000000011
	////00000000000000000000000000000101
	////00000000000000000000000000000001
	//printf("%d", c);
	//| - 按2进制位或
	//int a = 3;
	//int b = 5;
	//int c = a | b;
	////00000000000000000000000000000011
	////00000000000000000000000000000101
	////00000000000000000000000000000111
	//printf("%d", c);
	//^ - 按2进制位异或
	int a = 3;
	int b = 5;
	int c = a ^ b;
	//00000000000000000000000000000011
	//00000000000000000000000000000101
	//00000000000000000000000000000110 - 同0异1
	printf("%d", c);
	return 0;
}
